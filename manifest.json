{
  "manifest_version": 3,
  "name": "Blur Removal Extension",
  "version": "1.0",
  "description": "Removes blur effect from web pages",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "bypass.png"
  }
}
